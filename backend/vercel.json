{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma"],
        "buildCommand": "npm run prisma:generate"
      }
    }
  ],
  "postbuild": [
    {
      "run": "npx prisma generate"
    }
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
